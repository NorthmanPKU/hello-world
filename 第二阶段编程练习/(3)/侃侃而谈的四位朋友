// ConsoleApplication2.cpp : 定义控制台应用程序的入口点。
//

#include "stdafx.h"
#include<iostream>
using namespace std;
int main()
{
	int i11 =0, i12 = 0,i21=0,i22=0,i31=0,i32=0,i4=0;
	int l[4] = { 2,3,5,7 };
	int r[4] = { 0,0,0,0 };
	for ( i11 = 0; i11 < 4; i11++) {
		for ( i12 = i11 +1 ; i12 < 4; i12++) {
			for ( i21 = 0; i21 < 4; i21++) {
				for (i22 = i21 + 1; i22 < 4; i22++) {
					for (i31 = 0; i31 < 4; i31++) {
						for (i32 = i31 + 1; i32 < 4; i32++) {
							for (i4 = 0; i4 < 4; i4++) {
								r[0] = l[i11]*l[i12];
								r[1] = l[i21]*l[i22];
								r[2] = l[i31]*l[i32];
								r[3] = l[i4];
								//cout << 'w';
								//cout << "ren:" << r[0] << " " << r[1] << " " << r[2] << " " << r[3] << endl;
								if (((((r[0] + r[1] + r[2]) % 2 == 0) || ((r[0] + r[1] + r[2]) % 3 == 0) || ((r[0] + r[1] + r[2]) % 5 == 0) || ((r[0] + r[1] + r[2]) % 7 == 0)) ||
									(((r[3] + r[1] + r[2]) % 2 == 0) || ((r[3] + r[1] + r[2]) % 3 == 0) || ((r[3] + r[1] + r[2]) % 5 == 0) || ((r[3] + r[1] + r[2]) % 7 == 0)) ||
									(((r[0] + r[3] + r[2]) % 2 == 0) || ((r[0] + r[3] + r[2]) % 3 == 0) || ((r[0] + r[3] + r[2]) % 5 == 0) || ((r[0] + r[3] + r[2]) % 7 == 0)) ||
									(((r[0] + r[1] + r[3]) % 2 == 0) || ((r[0] + r[1] + r[3]) % 3 == 0) || ((r[0] + r[1] + r[3]) % 5 == 0) || ((r[0] + r[1] + r[3]) % 7 == 0))) &&
									(r[0]%7==0) && (r[3] % 7 != 0) && (r[1] % 3 != 0) && (r[0] * r[2] == 210) && (r[2] * r[3] == 30 || r[2] * r[3] == 105 || r[2] * r[3] == 70 || r[2] * r[3] == 42) &&
									(r[1] * r[2] % 4 == 0 || r[1] * r[2] % 9 == 0 || r[1] * r[2] % 25 == 0 || r[1] * r[2] % 49 == 0) && (r[0] != 35) && (r[1] != 35) && (r[2] != 35 )) {
									//cout << "yyy";
									goto uu;
									
								}
							}
						}
					}
				}
			}
		}
	}
	//cout << i11 << i12 << i21 << i22 << i31 << i32 << i4<<endl;
	//cout << "kk";
	//cout << "ren:"<<r[0]<<" " << r[1]<<" " << r[2]<<" " << r[3]<<endl;
uu:
	//cout << 6 % 7==0;
	for (int q = 0; q < 3; q++)
	{
		if (r[q] % 2 == 0) { r[q] /= 2; cout << 1 << " "; }
		else if (r[q] % 3 == 0) { r[q] /= 3; cout << 2 << " "; }
		else if (r[q] % 5 == 0) { r[q] /= 5; cout << 3 << " "; }
		else if (r[q] % 7 == 0) { r[q] /= 7; cout << 4 << " "; }
		if (r[q] % 2 == 0) cout << 1 << endl;
		else if (r[q] % 3 == 0) cout << 2 << endl;
		else if (r[q] % 5 == 0) cout << 3 << endl;
		else if (r[q] % 7 == 0) cout << 4 << endl;
	}
	switch (r[3]) {
	case 2:cout << 1; break;
	case 3:cout << 2; break;
	case 5:cout << 3; break;
	case 7:cout << 4; break;
	}
	
	system("pause");
return 0;
}

